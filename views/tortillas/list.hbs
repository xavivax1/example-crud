<h1>Our tortillas</h1>

<ul>
  {{#each tortillas}}
    <li> 
      <a href="/tortillas/{{this._id}}">{{this.name}}</a>
    </li>
  {{/each}}
</ul>

<form method="GET" id="search-tortilla" action="/api/tortillas">

  <div class="field" >
    <label>FIND BY USERNAME</label>
    <input type="text" name="username"/>
    
  </div>
  <div class="field submit">
    <button type="submit">Search</button>
  </div>
</form>

<script src="/javascript/index.js"></script>